<table>
  <thead>
    <tr>
      <th>Projet</th>
      <th>Description</th>
      <th>Liens</th>
    </tr>
  </thead>
  <tbody>
    {% for projet in collections.projets %}
    <tr>
      <td><a href="{{ projet.url }}">{{ projet.data.title }}</a></td>
      <td>{{ projet.data.description}}</td>
      <td>
        <a href="{{ projet.data.repos }}">
          <ion-icon name="logo-github"></ion-icon>
        </a>
        <a href="{{ projet.websiteUrl }}">
          <ion-icon name="link-outline"></ion-icon>
        </a>
      </td>
    </tr>
    {% endfor %}
  </tbody>
</table>

